#!/usr/bin/env bash
set -e

README="$(cat README.md)"
README="${README//"$PREVIOUS_VERSION/ramda.min.js"/"$VERSION/ramda.min.js"}"
README="${README//"${PREVIOUS_VERSION%.*}/ramda.min.js"/"${VERSION%.*}/ramda.min.js"}"
echo "$README" >README.md
git add README.md

make ramda.min.js
git add ramda.min.js
